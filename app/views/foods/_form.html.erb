<%= render "layouts/error_messages" %>
<%= form_with model: @food, local: true do |form| %>
  <div>
    <%= form.label :name, "食品" %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :comment, "コメント" %>
    <%= form.text_field :comment %>
  </div>
  <div>
    <%= form.submit button_value %>
  </div>
<% end %>
